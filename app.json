{
  "expo": {
    "name": "EcoYatra",
    "slug": "ecoyatra",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/icon.png",
    "platforms": ["ios", "android", "web"],

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "package": "com.giet.grievance.portal",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#F9FAFB"
      },
      "permissions": [],
      "softwareKeyboardLayoutMode": "pan"
    },

    "web": {
      "bundler": "metro"
    },

    "updates": {
      "enabled": false,
      "checkAutomatically": "ON_ERROR_RECOVERY"
    },

    "plugins": ["expo-router"],

    "experiments": {
      "typedRoutes": true
    },

    "extra": {
      "router": {},
      "eas": {
        "projectId": "8e8bc6b2-02fc-4e2b-a5c4-83194f460bbd"
      }
    }
  }
}
